{
  "name": "LineMonster",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "lint": "eslint --ext .js,.vue app/javascript",
    "lint-fix": "eslint --fix --ext .js,.vue app/javascript",
    "doc": "apidoc -i doc/api -o doc/public",
    "build": "node esbuild.config.mjs",
    "build:css:compile": "sass ./app/assets/stylesheets/application.bootstrap.scss:./app/assets/builds/application.css --no-source-map --load-path=node_modules",
    "build:css:prefix": "postcss ./app/assets/builds/application.css --use=autoprefixer --output=./app/assets/builds/application.css",
    "build:css": "npm run build:css:compile && npm run build:css:prefix",
    "watch:css": "nodemon --watch ./app/assets/stylesheets/ --ext scss --exec \"npm run build:css\""
  },
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "^16.0.0",
    "@ckeditor/ckeditor5-vue2": "^1.0.5",
    "@line/liff": "^2.15.0",
    "@rails/actioncable": "^6.0.0",
    "@rails/activestorage": "^6.0.0",
    "@rails/ujs": "^6.1.4-1",
    "axios": "^0.21.2",
    "bootstrap-vue": "^2.17.3",
    "chart.js": "^2.9.4",
    "chartkick": "^3.2.1",
    "d3": "^6.3.1",
    "dotenv": "^8.2.0",
    "lodash": "^4.17.21",
    "luxon": "^1.25.0",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.32",
    "toastr": "^2.1.4",
    "v-clipboard": "^2.2.3",
    "v-tooltip": "^2.0.3",
    "v-viewer": "^1.6.4",
    "vee-validate": "^2.2.15",
    "vee-validate-locale-ja": "^1.0.5",
    "vue": "^2.6.12",
    "vue-chartkick": "^0.6.1",
    "vue-click-outside": "^1.1.0",
    "vue-datetime": "^1.0.0-beta.14",
    "vue-google-oauth2": "^1.5.8",
    "vue-lazyload": "^1.3.3",
    "vue-loader": "^15.9.5",
    "vue-multiselect": "^2.1.6",
    "vue-qrcode-component": "^2.1.1",
    "vue-select": "^3.11.2",
    "vue-template-compiler": "^2.6.12",
    "vue2-daterange-picker": "^0.6.2",
    "vue2-google-maps": "^0.10.7",
    "vuex": "^3.5.1",
    "vuex-persistedstate": "^3.1.0",
    "weekstart": "^1.0.1"
  },
  "devDependencies": {
    "apidoc": "^0.25.0",
    "autoprefixer": "^10.4.21",
    "esbuild": "^0.25.5",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "sass": "^1.89.2"
  },
  "browserslist": [
    "defaults"
  ]
}
